{
  "swagger": "2.0",
  "info": {
    "title": "My API for TMS Delivery",
    "description": "Description",
    "version": "1.0.0"
  },
  "host": "localhost:3000/api/tms",
  "basePath": "/",
  "tags": [
    {
      "name": "Tms",
      "description": "Endpoints related to user as well as account operations"
    }
  ],
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "apiKeyAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "Authorization",
      "description": "Please enter JWT with Bearer into field"
    }
  },
  "paths": {
    "/register-tms": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Create account tms to get order when it created",
        "description": "Create account to get access token as well as website know which tms is perform",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Some informations about order",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "duchai"
                },
                "password": {
                  "type": "string",
                  "example": "123456"
                },
                "main_url": {
                  "type": "string",
                  "example": "http://localhost:3000"
                },
                "sub_url": {
                  "type": "string",
                  "example": "http://localhost:4000"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "created account tms successfully"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "some errors"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/update-status-order": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Update delivery status",
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Body information includes status (where 0 is cancel, 1 is prepare, 2 is shipping, 3 is customer reject, 4 is done), order_id is order code, delivery_notes is note",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "number",
                  "example": 4
                },
                "order_id": {
                  "type": "string",
                  "example": "666458f9d8f8927ab0cbf5f4"
                },
                "delivery_notes": {
                  "type": "string",
                  "example": "Giao hang thanh cong"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "update status order successfully"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Status is not valid"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/get-token": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Get access token to authorize api",
        "description": "Get token base on username ",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Some informations about account",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "duchai"
                },
                "password": {
                  "type": "string",
                  "example": "123456"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "get token successfully"
                },
                "data": {
                  "type": "string",
                  "example": "59495f06d24d6f1a9ada811eed0ae74d"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "some errors"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/get-order/{id}": {
      "get": {
        "tags": [
          "Tms"
        ],
        "summary": "Get order information by id",
        "description": "Get order information by id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Order id",
            "schema": {
              "type": "string",
              "example": "665fe8ccc5a09967910db414"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "get order successfully"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "delivery_information": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "example": "Jane Smith"
                        },
                        "phone_number": {
                          "type": "string",
                          "example": "0987654321"
                        },
                        "district": {
                          "type": "string",
                          "example": "District 3"
                        },
                        "ward": {
                          "type": "string",
                          "example": "Ward 5"
                        },
                        "street": {
                          "type": "string",
                          "example": "5678 Maple Avenue"
                        }
                      }
                    },
                    "_id": {
                      "type": "string",
                      "example": "666458f9d8f8927ab0cbf5f4"
                    },
                    "menu_detail": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "_id": {
                            "type": "number",
                            "example": 3
                          },
                          "name": {
                            "type": "string",
                            "example": "Chocolate Cake"
                          },
                          "quantity": {
                            "type": "number",
                            "example": 3
                          }
                        }
                      }
                    },
                    "shipping_fee": {
                      "type": "number",
                      "example": 20
                    },
                    "note": {
                      "type": "string",
                      "example": "Ring the doorbell twice"
                    },
                    "payment_method": {
                      "type": "string",
                      "example": "Cash"
                    },
                    "content": {
                      "type": "string",
                      "example": "Birthday celebration order"
                    },
                    "distance": {
                      "type": "string",
                      "example": "3 km"
                    },
                    "cod_amount": {
                      "type": "number",
                      "example": 150
                    },
                    "status": {
                      "type": "number",
                      "example": 0
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "some errors"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/order-list": {
      "get": {
        "tags": [
          "Tms"
        ],
        "summary": "Get order list",
        "description": "Get order list",
        "parameters": [
          {
            "name": "id",
            "description": "Order id",
            "schema": {
              "type": "string",
              "example": "665fe8ccc5a09967910db414"
            },
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "get order successfully"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "delivery_information": {
                        "type": "object",
                        "properties": {
                          "name": {
                            "type": "string",
                            "example": "Jane Smith"
                          },
                          "phone_number": {
                            "type": "string",
                            "example": "0987654321"
                          },
                          "district": {
                            "type": "string",
                            "example": "District 3"
                          },
                          "ward": {
                            "type": "string",
                            "example": "Ward 5"
                          },
                          "street": {
                            "type": "string",
                            "example": "5678 Maple Avenue"
                          }
                        }
                      },
                      "_id": {
                        "type": "string",
                        "example": "666458f9d8f8927ab0cbf5f4"
                      },
                      "menu_detail": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "_id": {
                              "type": "number",
                              "example": 3
                            },
                            "name": {
                              "type": "string",
                              "example": "Chocolate Cake"
                            },
                            "quantity": {
                              "type": "number",
                              "example": 3
                            }
                          }
                        }
                      },
                      "shipping_fee": {
                        "type": "number",
                        "example": 20
                      },
                      "note": {
                        "type": "string",
                        "example": "Ring the doorbell twice"
                      },
                      "payment_method": {
                        "type": "string",
                        "example": "Cash"
                      },
                      "content": {
                        "type": "string",
                        "example": "Birthday celebration order"
                      },
                      "distance": {
                        "type": "string",
                        "example": "3 km"
                      },
                      "cod_amount": {
                        "type": "number",
                        "example": 150
                      },
                      "status": {
                        "type": "number",
                        "example": 0
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "some errors"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    }
  }
}