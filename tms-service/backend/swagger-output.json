{
  "swagger": "2.0",
  "info": {
    "title": "My API for TMS Delivery",
    "description": "Description",
    "version": "1.0.0"
  },
  "host": "greenfeast.space/api/tms",
  "basePath": "/",
  "tags": [
    {
      "name": "Tms",
      "description": "Endpoints related to user as well as account operations"
    }
  ],
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "apiKeyAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "Authorization",
      "description": "Please enter JWT with Bearer into field"
    }
  },
  "paths": {
    "/update-status-order": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Update delivery status",
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Thông tin trong body bao gồm trạng thái của đơn hàng, 4: Đang giao hàng, 5: Đã giao hàng, 6: Đã hủy (Giao không thành công), 7: Trả món/Hoàn tiền, order_id là mã vận đơn, delivery_notes is ghi chú khi giao hàng",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "number",
                  "example": 4
                },
                "order_id": {
                  "type": "string",
                  "example": "666458f9d8f8927ab0cbf5f4"
                },
                "delivery_notes": {
                  "type": "string",
                  "example": "Giao hang thanh cong"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "update status order successfully"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Status is not valid"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        },
        "security": [
          {
            "apiKeyAuth": []
          }
        ]
      }
    },
    "/api/new-order": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Receive new order",
        "description": "Nhận đơn hàng mới đã sẵn sàng từ nhà hàng và shipper sẽ đến lấy hàng. Đơn vị vận chuyển sẽ cấu hình API này để nhận thông tin đơn hàng từ nhà hàng, đường dẫn đến API đã được cấu hình từ trước.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Thông tin đơn hàng mà nhà hàng cung cấp",
            "schema": {
              "type": "object",
              "properties": {
                "delivery_information": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "example": "Đức Hải"
                    },
                    "phone_number": {
                      "type": "string",
                      "example": "01231234"
                    },
                    "address": {
                      "type": "string",
                      "example": "Nguyễn Hữu Thọ"
                    },
                    "province": {
                      "type": "string",
                      "example": "Thành phố Hồ Chí Minh"
                    },
                    "district": {
                      "type": "string",
                      "example": "Quận 7"
                    },
                    "ward": {
                      "type": "string",
                      "example": "Phường Tân Phú"
                    },
                    "longitude": {
                      "type": "string",
                      "example": "106.7261695"
                    },
                    "latitude": {
                      "type": "string",
                      "example": "10.727068"
                    }
                  }
                },
                "order_person": {
                  "type": "object",
                  "properties": {
                    "_id": {
                      "type": "string",
                      "example": "0123456789"
                    },
                    "name": {
                      "type": "string",
                      "example": "Lưu Đức hải"
                    }
                  }
                },
                "_id": {
                  "type": "string",
                  "example": "66ac7e2ba4128887d7a8e7d5"
                },
                "menu_detail": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "number",
                        "example": 3
                      },
                      "name": {
                        "type": "string",
                        "example": "Đậu hũ chiên giòn"
                      },
                      "quantity": {
                        "type": "number",
                        "example": 1
                      },
                      "price": {
                        "type": "number",
                        "example": 30000
                      }
                    }
                  }
                },
                "subtotal": {
                  "type": "number",
                  "example": 120000
                },
                "discount": {
                  "type": "number",
                  "example": 24000
                },
                "surcharge": {
                  "type": "number",
                  "example": 0
                },
                "shippingfee": {
                  "type": "number",
                  "example": 15000
                },
                "note": {
                  "type": "string",
                  "example": ""
                },
                "total": {
                  "type": "number",
                  "example": 111000
                },
                "time": {
                  "type": "string",
                  "example": "2024-08-02T06:33:32.196Z"
                },
                "payment_method": {
                  "type": "string",
                  "example": "bank"
                },
                "status": {
                  "type": "number",
                  "example": 3
                },
                "is_rating": {
                  "type": "boolean",
                  "example": false
                },
                "__v": {
                  "type": "number",
                  "example": 0
                }
              }
            }
          }
        ],
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/register-tms": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Create account tms to get order when it created",
        "description": "Create account to get access token as well as website know which tms is perform",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Some informations about order",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "duchai"
                },
                "password": {
                  "type": "string",
                  "example": "123456"
                },
                "main_url": {
                  "type": "string",
                  "example": "http://localhost:3000"
                },
                "sub_url": {
                  "type": "string",
                  "example": "http://localhost:4000"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "created account tms successfully"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "some errors"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/get-token": {
      "post": {
        "tags": [
          "Tms"
        ],
        "summary": "Get access token to authorize api",
        "description": "Get token base on username ",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Some informations about account",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "duchai"
                },
                "password": {
                  "type": "string",
                  "example": "123456"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "get token successfully"
                },
                "data": {
                  "type": "string",
                  "example": "59495f06d24d6f1a9ada811eed0ae74d"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "some errors"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
